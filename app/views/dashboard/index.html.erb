<%= render partial: './navbar' %>
<section class="spacer">

</section>

<section class="m0 images col-6 mx-auto">
  <% @images.each do |image| %>
    <div class="mb1 p1 rounded image bg-soft-white">
      <h2 class="m0 center block"><%= image.park %></h2>
      <%= link_to image_tag("#{image.url}", class: "mx-auto block med-image rounded"), image.park_url, target: :_blank %>
      <p class="m1 center block"><%= image.caption %></p>
    </div>
  <% end %>
</section>
